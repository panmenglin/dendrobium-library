{
  "分页 Pagination": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "antd Pagination pagination 分页",
    "body": {
      "element": [
        "<Pagination",
        "  showSizeChanger",
        "  onChange={this.onChange}",
        "  current={this.state.current}",
        "  total={$2}",
        "/>"
      ],
      "state": [
        "state = {",
        "  current: 3",
        "}"
      ],
      "method": [
        "onChange = (page) => {",
        "  console.log(page);",
        "  this.setState({",
        "    current: page,",
        "  });",
        "}"
      ]
    },
    "description": "页码受控制的分页组件。",
    "componentCode": "antd/pagination",
    "libraryCode": "antd"
  }
}
