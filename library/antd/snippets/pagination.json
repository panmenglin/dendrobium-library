{
  "antd cascader": {
    "scope": "javascript,typescript,javascriptreact,typescriptreact",
    "prefix": "Cascader",
    "body": [
      "cascaderRender = () => {",
      "  const options = [",
      "    {",
      "      value: 'zhejiang',",
      "      label: 'Zhejiang',",
      "      children: [",
      "        {",
      "          value: 'hangzhou',",
      "          label: 'Hangzhou',",
      "          children: [",
      "            {",
      "              value: 'xihu',",
      "              label: 'West Lake',",
      "            },",
      "          ],",
      "        },",
      "      ],",
      "    },",
      "    {",
      "      value: 'jiangsu',",
      "      label: 'Jiangsu',",
      "      children: [",
      "        {",
      "          value: 'nanjing',",
      "          label: 'Nanjing',",
      "          children: [",
      "            {",
      "              value: 'zhonghuamen',",
      "              label: 'Zhong Hua Men',",
      "            },",
      "          ],",
      "        },",
      "      ],",
      "    },",
      "  ];",
      "",
      "  function onChange(value) {",
      "    console.log(value);",
      "  }",
      "",
      "  return (<Cascader",
      "    defaultValue={['zhejiang', 'hangzhou', 'xihu']}",
      "    options={options}",
      "    onChange={onChange}",
      "  />)",
      "}"
    ],
    "description": "级联选择",
    "componentCode": "antd/cascader",
    "libraryCode": "antd"
  }
}
